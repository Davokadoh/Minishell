var=salut
//Fonctionne
"var"=salut
//Ne fonctionne pas

var="Salut, ceci est un $test"
test=coucou
echo $var
//"Salut, ceci est un "

test=coucou
var="Salut, ceci est un $test"
echo $var
//"Salut, ceci est un coucou"

cat file1 | <file2 cat
//Only prints file2

echo 'Hello' | cat < file1 file2
//Only prints file2

cat file cat cat
//the first cat prints file,cat,cat
//the catch here is that we have a file named cat
//or, we have only one command started, not 2, nor 3.

FAKE_CMD
echo $? | cat
//Should print 127, as FAKE_CMD does not exists

FAKE_CMD
echo $? | cat | echo $?
//Should print 0, as cat worked correctly

exit | wc

wc | exit

export VAR='"hello"'
echo $VAR
//Should print "hello", not just hello
